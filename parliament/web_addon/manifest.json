{
  "manifest_version": 2,
  "name": "Zinneke Babeleir Addon",
  "version": "0.1",

  "description": "For the zinneke babeleir site, fill in the vote",

  "permissions": [
    "activeTab"
  ],

  "icons": {
    "48": "index.ico"
  },

  "browser_action": {
    "default_title": "Zinneke Babeleir",
    "default_popup": "popup/popup.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*.zinneke-babeleir.be/*", "*://*.127.0.0.1/*"]
    }
  ]
}